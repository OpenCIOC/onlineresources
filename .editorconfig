# top-most EditorConfig file
root = true
tab_width = 4

# Unix-style newlines with a newline ending every file
[*]
insert_final_newline = true
trim_trailing_whitespace = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{js,asp}]
charset = utf-8-bom

[*.py]
indent_style = space
indent_size = 4
charset = utf-8

[*.asp]
indent_style = tab
