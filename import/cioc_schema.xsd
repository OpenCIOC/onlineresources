	<xsd:complexType name="c-ChecklistType">
		<xsd:sequence>
			<xsd:element name="CHK" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:attribute name="CD" type="s-CodeBasicType" use="optional"/>
					<xsd:attribute name="V" type="s-Text200Type" use="required"/>
					<xsd:attribute name="VF" type="s-Text200Type" use="optional"/>
					<xsd:attribute name="N" type="s-Text255Type" use="optional"/>
					<xsd:attribute name="NF" type="s-Text255Type" use="optional"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="N" type="s-Text4000Type" use="optional"/>
		<xsd:attribute name="NF" type="s-Text4000Type" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="c-ChecklistTypeExtra">
		<xsd:sequence>
			<xsd:element name="CHK" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
