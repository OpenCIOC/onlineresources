	<xsd:simpleType name="s-URLType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="((\d{1,3}(\.\d{1,3}){3})|([\w_-]+(\.[\w\._-]+)*))(:[0-9]+)?([/?][^\s]*)?"/>
			<xsd:minLength value="1"/>
			<xsd:maxLength value="200"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="s-URLWithProtocolType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="(http://|https://)((\d{1,3}(\.\d{1,3}){3})|([\w_-]+(\.[\w\._-]+)*))(:[0-9]+)?([/?][^\s]*)?"/>
			<xsd:minLength value="1"/>
			<xsd:maxLength value="200"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="s-URLProtocolType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="http://"/>
			<xsd:enumeration value="https://"/>
		</xsd:restriction>
	</xsd:simpleType>
